{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "api:check": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "api:update": {
      "dependsOn": ["^build"]
    }
  }
}
